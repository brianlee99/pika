exec {
	const x := "hello".
	const y := "hello".
	const z := "Hello".
	const w := "hello ".
	
	print x != z; _n_.  # true
	print x == y; _n_.  # true
	print x != y; _n_.  # false
	print y == z; _n_.  # false
	print y == w; _n_.  # false
	print w == x; _n_.  # false
