exec {
    const a := [1,2,3].
    var b   := a.
    
    print a[0];a[1];a[2],_n_.
    
    b[0]    := 6.
    b[1]    := 7.
    b[2]    := 8.
    print a[0];a[1];a[2],_n_.
    
    a[0]    := 5.
    a[1]    := 5.
    a[2]    := 5.
    print b[0];b[1];b[2],_n_.
    
    b := [0,0,0].
    print a[0];a[1];a[2],_n_.
}
